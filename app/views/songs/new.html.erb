<%= form_for @song, url: { action: "create" } do |f| %>
  <%= f.label :name %><br>
  <%= f.text_field :name %><br>
  <%= f.label :artist %><br>
  <%= collection_select(@song[:artist_id], @artists, :id, :name) %><br>
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <%= f.submit "Create Song", html: { class: "btn-submit" } %>
<% end %>
<br>
